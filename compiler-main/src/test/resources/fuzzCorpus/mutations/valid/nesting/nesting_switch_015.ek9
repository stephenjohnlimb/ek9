              switch value
                case > 2
                  switch value
                    case > 3
                      switch value
                        case > 4
                          switch value
                            case > 5
                              switch value
                                case > 6
                                  switch value
                                    case > 7
                                      switch value
                                        case > 8
                                          switch value
                                            case > 9
                                              result: 10
