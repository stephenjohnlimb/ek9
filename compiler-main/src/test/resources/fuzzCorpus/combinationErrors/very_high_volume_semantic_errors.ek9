      //Section 3: Type incompatibilities (20 errors: 10 NOT_REFERENCED + 10 INCOMPATIBLE_TYPES)
      val1 <- 42
      val2 <- "hello"
      val3 <- true
      val4 <- 3.14
      val5 <- Date()

      @Error: FULL_RESOLUTION: NOT_REFERENCED
