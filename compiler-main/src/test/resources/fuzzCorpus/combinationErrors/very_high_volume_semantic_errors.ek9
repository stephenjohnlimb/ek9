      @Error: FULL_RESOLUTION: NOT_RESOLVED
      obj7 <- UnknownType7()

      @Error: FULL_RESOLUTION: NOT_RESOLVED
      obj8 <- UnknownType8()

      @Error: FULL_RESOLUTION: NOT_RESOLVED
      obj9 <- UnknownType9()

      @Error: FULL_RESOLUTION: NOT_RESOLVED
      obj10 <- UnknownType10()

      //Section 2: Unused variables (10 errors)
