#!ek9
<?-
  Runtime fuzz test: Synthetic :=: operator - copy to empty destination.

  Tests that when copying to an empty destination object,
  the destination receives all values from the source.
-?>
defines module fuzz.runtime.synthetic.copy.toempty

  defines class

    TestPoint
      x <- Integer()
      y <- Integer()

      TestPoint()
        ->
          xVal as Integer
          yVal as Integer
        this.x :=: xVal
        this.y :=: yVal

      default operator ?
      default operator $
      default operator :=:

  defines program

    SyntheticCopyToEmpty()
      stdout <- Stdout()

      stdout.println("Test: Synthetic :=: copy to empty destination")

      //Create fully set source
      src <- TestPoint(42, 84)

      //Create empty destination (not set)
      dst <- TestPoint()

      stdout.println("Before copy:")
      stdout.println("src: " + $src)
      stdout.println("dst: " + $dst)
      stdout.println("src?: " + $src?)
      stdout.println("dst?: " + $dst?)

      //Copy to empty destination
      dst :=: src

      stdout.println("After copy:")
      stdout.println("src: " + $src)
      stdout.println("dst: " + $dst)
      stdout.println("dst?: " + $dst?)

      stdout.println("Test complete")

//EOF
