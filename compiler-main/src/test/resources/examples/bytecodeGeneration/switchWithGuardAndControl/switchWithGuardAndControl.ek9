    SwitchWithGuardAndControl()
      -> input as Integer
      stdout <- Stdout()
      message <- "Default"

      // Simulating declaration guard (<-) for switch with control
      exprValue <- Integer()
      if input > 0
        exprValue: Integer(input)

      // Switch with guard - only runs if exprValue is set
      switch value <- exprValue
        case 1
          message: "One"
        case 2
          message: "Two"
        default
          message: "Other: " + $value

      stdout.println("Result: " + message)
