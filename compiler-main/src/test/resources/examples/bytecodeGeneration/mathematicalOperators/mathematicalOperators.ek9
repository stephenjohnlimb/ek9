      stdout.println(rem1)

      // Test power (^)
      pow1 <- c ^ b      // 8 (2^3)
      stdout.println(pow1)

      // Test absolute (abs)
      abs1 <- abs neg    // 10
      stdout.println(abs1)

      // Test square root (sqrt) - returns Float
      sqrt1 <- sqrt 16   // 4.0
      stdout.println(sqrt1)

      // Test factorial (!)
