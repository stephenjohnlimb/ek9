    ForInAssignmentIfUnset()
      -> input as Integer
      stdout <- Stdout()
      total <- 0

      // Simulating assignment if unset (:=?) pattern for for-in
      // Create list, only populate if input > 0
      items <- List() of String
      if input > 0
        items += "one"
        items += "two"
        items += "three"

      // For-in with empty list just doesn't iterate
      for item in items
        total := total + 1
        stdout.println("item=" + item)

      stdout.println("Total: " + $total)
