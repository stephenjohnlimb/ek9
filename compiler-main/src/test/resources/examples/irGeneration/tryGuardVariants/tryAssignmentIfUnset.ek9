    tryAssignmentIfUnset()
      stdout <- Stdout()

      // Declare variable first (unset)
      config <- String()

      // Assignment if unset (:=?) - checks LEFT side, only assigns if unset
      // Guard entry check happens ONCE, try block runs only if guard succeeds
      try config :=? getConfig()
        stdout.println("Config loaded:")
        stdout.println(config)
      catch
        -> ex as Exception
        stdout.println("Failed to load config")
      finally
        stdout.println("Cleanup complete")

