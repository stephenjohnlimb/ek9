RETURN`
    doWhileGuardedAssignment()
      <- counter <- Integer()

      // Declare variable first (unset)
      value <- Integer()

      // Guarded assignment (?=) - checks if assigned value is set
      // Guard entry check happens ONCE, body first, then condition
      do value ?= getValue()
        counter := counter + 1
      while value > counter

