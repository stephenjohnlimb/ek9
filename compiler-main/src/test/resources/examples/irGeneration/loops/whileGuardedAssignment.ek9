    whileGuardedAssignment()
      <- counter <- Integer()

      // Declare variable first (unset)
      value <- Integer()

      // Guarded assignment (?=) - checks if assigned value is set
      // Guard entry check happens ONCE, condition on each iteration
      while value ?= getValue() then value > counter
        counter := counter + 1

