    switchAssignmentIfUnset()
      <- result <- String()

      // Declare variable first (unset)
      value <- Integer()

      // Assignment if unset (:=?) - checks LEFT side, only assigns if unset
      switch value :=? getValue()
        case 1
          result: "One"
        case 2
          result: "Two"
        default
          result: "Other"

      // Reference value to satisfy unreferenced variable check
      assert value?

//EOF
