    switchAssignmentGuard()
      <- result <- String()

      // Declare variable first (unset)
      value <- Integer()

      // Assignment guard (:=) - blind assignment, NO guard check
      switch value := getValue()
        case 1
          result: "One"
        case 2
          result: "Two"
        default
          result: "Other"

      // Reference value to satisfy unreferenced variable check
      assert value?

