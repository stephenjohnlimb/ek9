BasicBlock: _entry_1
RETURN`
    simpleSwitchFloat()
      -> value as Float
      <- result <- String()

      switch value
        case < -10.0
          result := "Very negative"
        case <= 0.0
          result := "Zero or negative"
        case < 10.0
          result := "Low"
        case >= 100.0
          result := "Very high"
        case > 50.0
          result := "High"
        default
          result := "Medium"

//EOF
