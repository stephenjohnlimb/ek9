    RETAIN _temp35
    SCOPE_REGISTER _temp35, _scope_6
    _temp34 = CALL (org.ek9.lang::Integer)_temp35._string() [pure=true, complexity=1, effects=RETURN_MUTATION]
    RETAIN _temp34
    SCOPE_REGISTER _temp34, _scope_6
    _temp32 = CALL (org.ek9.lang::String)_temp33._add(_temp34) [pure=true, complexity=2, effects=RETURN_MUTATION]
