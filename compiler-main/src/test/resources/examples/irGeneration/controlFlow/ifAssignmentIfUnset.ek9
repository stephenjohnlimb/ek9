    ifAssignmentIfUnset()
      <-
        result <- String()

      // Declare variable first (unset)
      existing <- Integer()

      // Assignment if unset (:=?) - checks LEFT side, only assigns if unset
      // THEN checks if result is set (guard check)
      // This is lazy evaluation - don't evaluate expr if not needed
      // Different from ?= which always evaluates
      if existing :=? supplyInteger() with existing > 10
        result: "High"
      else
        result: "Low"

