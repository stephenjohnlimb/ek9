RETURN`
    ifGuardedAssignment()
      <-
        result <- String()

      // Declare variable first (unset)
      existing <- Integer()

      // Guarded assignment (?=) - checks if assigned value is set
      // This is similar to <- but for existing variables
      // Guard check verifies the assignment succeeded
      if existing ?= provideOptional() with existing > 10
        result: "High"
      else
        result: "Low"

