<?-
  Workarea for testing during development.
-?>
defines module workarea

  defines function

    multipleCaseCharacterPromotion()
      -> text as String
      <- result <- String()

      switch text
        case 'D', 'A', 'Z'
          result := "Found vowel or D"
        case 'X', 'Y'
          result := "Found X or Y"
        default
          result := "Other letter"

