<?-
  Tests STREAM_TYPE_NOT_DEFINED error.
  Stream pipeline with function that returns Void.
  Void cannot be used in stream pipelines.
-?>
defines module bad.stream.types

  defines function
    voidFunction()
      -> arg as String
      assert arg?
      //No return value - returns Void

    testStreamWithVoid()
      values <- ["a", "b", "c"]
      @Error: FULL_RESOLUTION: STREAM_TYPE_NOT_DEFINED
      result <- cat values | map voidFunction | collect as Void

//EOF
